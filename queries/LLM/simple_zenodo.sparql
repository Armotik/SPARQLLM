# slm-run --config config.ini -f queries/LLM/simple_zenodo.sparql --debug
PREFIX ex: <http://example.org/>
PREFIX ggf: <http://ggf.org/>
SELECT *  WHERE {
    BIND(ggf:SLM-FILE("./data/zenodo.html") AS ?file)
    BIND(ggf:SLM-READHTMLFILE(?file,1000) AS ?page) 
    BIND(CONCAT("""
        return as JSON-LD  the schema.org representation of text below. 
        Only respond with valid JSON-LD : """,
        STR(?page)) AS ?prompt) 
    BIND(ggf:SLM-LLMGRAPH(?prompt,ex:myentity) AS ?g)
    GRAPH ?g {
        ?uri <http://example.org/has_schema_type> ?o 
    }    
}