# run with:
# slm-run --config config.ini -f queries/mcp/github-no-mcp.sparql --debug

PREFIX ggf:    <http://ggf.org/>
PREFIX schema: <https://schema.org/>
PREFIX prov:   <http://www.w3.org/ns/prov#>

SELECT ?pr ?title ?time  WHERE {
  BIND( ggf:GITHUB("twitter", "the-algorithm") AS ?g )
  GRAPH ?g { 
    ?repo rdf:type schema:Repository ;
          schema:hasPart ?pr .
          ?pr schema:name ?title .
    ?g prov:generatedAtTime ?time .
  }
} LIMIT 5
