##
## run with:
## slm-run --config config.ini -f queries/filesystem/simple_publi.sparql --debug
## should see hallucinations.

PREFIX dct: <http://purl.org/dc/terms/>
PREFIX ex: <http://example.org/>
PREFIX ggf: <http://ggf.org/>
        
SELECT ?pub ?journal ?llm
 WHERE {
    BIND(ggf:SLM-FILE("./data/publications.rdf") AS ?uri)
    BIND(ggf:SLM-RDF(?uri,"xml") AS ?g)
    graph ?g {
        ?pub dct:isPartOf ?part .
        ?part dc:title ?journal .
    }
} limit 10 
